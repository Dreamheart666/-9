#include <stdio.h>
#include <windows.h>
#include<string.h>
#include <math.h>
#include<windows.h>
//创建一个结构体类型struct Stu 
struct Stu
{
	//成员变量
	char name[20];
	int age;
	char id[20];
};
int main()
{
	int a = 20;
	//使用struct Stu这个类型创建一个学生对象s1，并初始化
	struct Stu s1 = { "张三", 20, "2022202080011" };
	printf("%d\n", s1.age);
	printf("%s\n", s1.id);
	printf("%s\n", s1.name);
	//结构体变量.成员名
	return 0;
}
